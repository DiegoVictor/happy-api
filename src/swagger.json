{
  "openapi":"3.0.2",
  "info": {
    "title":"Happy",
    "version":"1.0",
    "license": {
      "name": "MIT",
      "url": "https://github.com/DiegoVictor/happy-api/blob/master/LICENSE"
    }
  },
  "servers": [
    {
      "url":"http://localhost:3333/v1"
    }
  ],
  "paths": {
    "/orphanages": {
      "get": {
        "tags": ["orphanages"],
        "responses": {
          "200": {
            "description": "Lists orphanages",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Orphanage"
                  }
                },
                "examples": {
                  "Orphanage": {
                    "$ref": "#/components/examples/Orphanages"
                  }
                }
              }
            }
          }
        }
      },
  },
  "components": {
    "schemas": {
      "Orphanage": {
        "properties": {
          "id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "latitude": {
            "type": "number"
          },
          "longitude": {
            "type": "number"
          },
          "about": {
            "type": "string"
          },
          "instructions": {
            "type": "string"
          },
          "opening_hours": {
            "type": "string"
          },
          "open_on_weekends": {
            "type": "boolean"
          },
          "whatsapp": {
            "type": "string"
          },
          "images": {
            "type": "array",
            "items": {
              "properties": {
                "id": {
                  "type": "integer"
                },
                "path": {
                  "type": "string"
                }
              }
            }
          },
          "url": {
            "type": "string"
          }
        }
      },
    },
  }
}
